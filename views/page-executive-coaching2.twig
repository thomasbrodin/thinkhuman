{% extends "base.twig" %}

{% block content %}

	<div id="{{post.post_type}}">
        <div class="container">
          <div class="row">
          	
          	<div class="col-md-12">
				<h1 class="title">{{post.title}}</h1>
				<ul class="coaching-menu">
				    {% for child in post.children %}
				        <li>
				        	<a href="{{child.link}}">{{child.title}}</a>
				        </li>
				    {% endfor %}
				</ul>
				<a href="/services">Back to our Services</a>
          	</div>

        </div>
      </div><!--content -->
	</div>

	<!--   Clients logos pre Footer       -->
	{% include 'partials/bottom-page.twig' %}

{% endblock %}