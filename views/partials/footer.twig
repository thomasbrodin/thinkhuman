				  	<a href="{{options.linkedin}}" class="social-connect linkdin"></a>
				  {% endif %}
				  {% if options.email %}
				  	<a href="{{options.email}}" class="social-connect email"></a>
				   {% endif %}
				</div>
			</div>
	</footer>

	{{ function('wp_footer') }}
